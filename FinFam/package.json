{
  "name": "finfam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --config ./jest.config.js --detectOpenHandles",
    "test:frontend": "jest --config ./jest.config.frontend.js --watchAll=false",
    "test:backend": "jest --config ./jest.config.js --detectOpenHandles",
    "test:all": "npm run test:backend && npm run test:frontend",
    "test:coverage": "jest --config ./jest.config.js --coverage && jest --config ./jest.config.frontend.js --coverage",
    "test:watch": "jest --config ./jest.config.frontend.js --watch",
    "server": "ts-node server/server.ts",
    "server:dev": "nodemon --exec ts-node server/server.ts",
    "start": "npm run build && npm run server",
    "migrate:generate": "drizzle-kit generate:pg",
    "migrate:push": "drizzle-kit push:pg",
    "migrate:drop": "drizzle-kit drop",
    "migrate:check": "drizzle-kit check:pg",
    "migrate:up": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@tanstack/react-query": "^4.36.1",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-flash": "^0.0.40",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react-datepicker": "^4.19.5",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "compression": "^1.8.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "node-cache": "^5.1.2",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.3.6",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.8.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.5",
    "vite-plugin-compression2": "^0.11.0"
  }
}
